.room-object {

    position: absolute
    cursor: pointer
    z-index: 2

    &_selected_yes {
        box-shadow: 0px 0px 5px 2px #F9D800;
    }

    &__label {
        padding: 2px;
        user-select: none
        img {
            vertical-align: middle
            margin-right: 2px
        }
        span {
            font-size: 14px
            color: #333
        }
    }

}
